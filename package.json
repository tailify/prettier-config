{
  "name": "@tailify/prettier-config",
  "version": "1.0.0",
  "description": "Tailify's very own opinionated Prettier configuration",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint && npm run test-only",
    "test-only": "jest"
  },
  "repository": "tailify/prettier-config",
  "keywords": [
    "prettier",
    "prettier-config"
  ],
  "license": "UNLICENSED",
  "bugs": "https://github.com/tailify/prettier-config/issues",
  "homepage": "https://github.com/tailify/prettier-config#readme",
  "devDependencies": {
    "@tailify/eslint-config-legacy": "^2.0.0",
    "eslint": "^5.12.0",
    "jest": "^24.7.0",
    "prettier": "^1.15.0"
  },
  "peerDependencies": {
    "prettier": "^1.15.0"
  },
  "engines": {
    "node": ">=8"
  },
  "greenkeeper": {
    "commitMessages": {
      "dependencyUpdate": "Update ${dependency} to version ${version}",
      "devDependencyUpdate": "Update ${dependency} to version ${version}",
      "dependencyPin": "Pin ${dependency} to ${oldVersion}",
      "devDependencyPin": "Pin ${dependency} to ${oldVersion}"
    },
    "prTitles": {
      "basicPR": "Update ${dependency} to the latest version",
      "groupPR": "Update ${dependency} in group ${group} to the latest version"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
